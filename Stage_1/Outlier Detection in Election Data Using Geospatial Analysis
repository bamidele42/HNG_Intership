     "data": {
      "text/plain": [
       "(18, 13)"
      ]
     },
     "execution_count": 273,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Outlier detection for PDP\n",
    "Q1 = buffered.PDP.quantile(0.25)\n",
    "Q3 = buffered.PDP.quantile(0.75)\n",
    "Q1, Q3\n",
    "IQR = Q3 - Q1\n",
    "IQR\n",
    "\n",
    "min_threshold_pdp = Q1 - 1.5*IQR\n",
    "max_threshold_pdp = Q3 + 1.5*IQR\n",
    "min_threshold_pdp, max_threshold_pdp\n",
    "\n",
    "buffered[buffered[\"PDP\"] > max_threshold_pdp].shape"
   ]
  },
